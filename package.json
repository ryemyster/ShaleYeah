{
    "name": "shale-yeah",
    "version": "0.1.0",
    "description": "Democratize oil & gas investing through AI agent flows that replace 100+ employees",
    "private": false,
    "type": "module",
    "main": "dist/main.js",
    "scripts": {
        "build": "tsc",
        "dev": "NODE_ENV=development tsx src/main.ts",
        "start": "NODE_ENV=production tsx src/main.ts",
        "init": "npx claude-flow@alpha init",
        "gen": "tsx scripts/generate-from-spec.ts",
        "demo": "npx tsx src/demo-runner.ts",
        "demo:enhanced": "npx tsx src/enhanced-demo-runner.ts",
        "prod": "npx tsx src/main.ts --mode=production",
        "pipeline": "tsx src/main.ts",
        "pipeline:demo": "npx tsx src/demo-runner.ts",
        "pipeline:prod": "NODE_ENV=production tsx src/main.ts --mode=production",
        "pipeline:batch": "NODE_ENV=production tsx src/main.ts --mode=batch",
        "pipeline:research": "NODE_ENV=development tsx src/main.ts --mode=research",
        "lint": "biome check . || true",
        "type-check": "tsc --noEmit",
        "clean": "npm run clean:dist && npm run clean:outputs && npm run clean:cache",
        "clean:dist": "rm -rf dist/",
        "clean:outputs": "rm -rf data/outputs/*",
        "clean:cache": "rm -rf node_modules/.cache .tsx-cache",
        "clean:all": "npm run clean && rm -rf node_modules/ package-lock.json",
        "test": "npx tsx tests/demo.test.ts",
        "test:mcp": "npx tsx scripts/test-geowiz-standalone.ts",
        "server:geowiz": "npx tsx src/mcp-servers/geowiz-standalone.ts",
        "server:econobot": "npx tsx src/mcp-servers/econobot-standalone.ts"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.30.1",
        "@modelcontextprotocol/sdk": "^1.17.3",
        "@turf/turf": "^6.5.0",
        "dotenv": "^16.4.5",
        "openai": "^5.15.0",
        "proj4": "^2.9.0",
        "shapefile": "^0.6.6",
        "xlsx": "^0.18.5",
        "xml2js": "^0.6.2",
        "yaml": "^2.6.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.2",
        "@types/xml2js": "^0.4.14",
        "tsx": "^4.20.5",
        "typescript": "^5.6.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "oil-gas",
        "investment",
        "ai-agents",
        "llm",
        "orchestration",
        "geological-analysis",
        "typescript"
    ],
    "author": "Ryan McDonald / Ascendvent LLC",
    "license": "Apache-2.0"
}
